import java.util.*;

public class p96
{

public static void main(String args[])
{
	Vector a=new Vector();
	String n;
	K k=new K();
	a.addElement("Hello! ");
	a.addElement("welcome to");
	a.addElement(" program");
	a.addElement(" world!");
	a.insertElementAt(" C",2);
	System.out.println(a.size());	
	
	k.Oout(a);
	//Oout(a);
	System.out.println();	
	
	a.setElementAt(" java",2);
	a.removeElementAt(3);
	System.out.println(a.size());
	
